<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <link rel="stylesheet" type="text/css" href="../../Styles/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="../../Styles/admin-all.css" />
    <script type="text/javascript" src="../../Scripts/jquery-1.7.2.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery-ui-1.8.22.custom.min.js"></script>
    <link rel="stylesheet" type="text/css" href="../../Styles/ui-lightness/jquery-ui-1.8.22.custom.css" />
    <script type="text/javascript">
        $(function () {
            $(".datepicker").datepicker();

            $('#list').hide();
            $('#find').click(function () {
                $('#list').show();
            })
        })

    </script>
    <style type="text/css">
<!--
.STYLE1 {
	font-family: "新宋体";
	font-weight: bold;
}
-->
    </style>
</head>
<body>
    <div class="alert alert-info">当前位置<b class="tip"></b>客户服务管理<b class="tip"></b>服务处理</div>
    <table class="table table-striped table-bordered table-condensed">
        
        <tbody>

            <tr>
              <td width="20%"><div align="center"><span class="STYLE1">编号</span></div></td>
              <td width="30%" class="detail">&nbsp;</td>
              <td width="20%"><div align="center"><span class="STYLE1">服务类型</span></div></td>
              <td width="30%" class="td_detail">&nbsp;</td>
            </tr>
            <tr>
                <td><div align="center"><span class="STYLE1">概要</span></div></td>
                <td colspan="3" class="detail">&nbsp;</td>
            </tr>
            <tr>
                <td><div align="center"><span class="STYLE1">客户</span></div></td>
                <td>&nbsp;</td>
                <td><div align="center"><span class="STYLE1">状态</span></div></td>
                <td>&nbsp;</td>
            </tr>
            <tr>
                <td><div align="center"><span class="STYLE1">服务请求</span></div></td>
                <td colspan="2">&nbsp;</td>
                <td colspan="3">&nbsp;</td>
            </tr>
            <tr>
                <td><div align="center"><span class="STYLE1">创建人</span></div></td>
                <td>&nbsp;</td>
                <td><div align="center"><span class="STYLE1">创建时间</span></div></td>
                <td colspan="3">&nbsp;</td>
            </tr>
        </tbody>
    </table>
	 <table class="table table-striped table-bordered table-condensed">
        
        <tbody>
            <tr>
                <td width="20%"><div align="center"><span class="STYLE1">分配给</span></div></td>
                <td width="30%">&nbsp;</td>
                <td width="20%"><div align="center"><span class="STYLE1">分配时间</span></div></td>
                <td width="30%">&nbsp;</td>
            </tr>
        </tbody>
    </table>
	 <table class="table table-striped table-bordered table-condensed">
        
        <tbody>
            <tr>
                <td width="20%"><div align="center"><span class="STYLE1">服务处理</span></div></td>
                <td colspan="3">&nbsp;</td>
            </tr>
            <tr>
                <td><div align="center"><span class="STYLE1">处理人</span></div></td>
                <td width="30%">&nbsp;</td>
                <td width="20%"><div align="center"><span class="STYLE1">处理时间</span></div></td>
                <td width="30%">&nbsp;</td>
            </tr>
        </tbody>
    </table>
	 <table class="table table-striped table-bordered table-condensed">
        
        <tbody>
            <tr>
                <td width="20%"><div align="center"><span class="STYLE1">处理结果 </span></div></td>
                <td width="30%">&nbsp;</td>
                <td width="20%"><div align="center"><span class="STYLE1">满意度</span></div></td>
                <td width="30%">&nbsp;</td>
            </tr>
        </tbody>
    </table>
   
</body>
</html>
